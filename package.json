{
  "name": "ct-azure-function-app-nodejs-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Azure Function App starter template",
  "scripts": {
    "functions": "cross-env NODE_ENV=development func start",
    "functions:verbose": "cross-env NODE_ENV=development func start --verbose",
    "test": "echo \"No tests yet...\""
  },
  "author": {
    "name": "Michael Sprague",
    "email": "ms388@cornell.edu"
  },
  "repository": {
    "url": "https://github.com/CU-CommunityApps/ct-azure-function-app-nodejs-starter"
  },
  "bugs": {
    "url": "https://github.com/CU-CommunityApps/ct-azure-function-app-nodejs-starter/issues"
  },
  "engines": {
    "node": "16.x",
    "npm": ">= 7.x"
  },
  "dependencies": {
    "axios": "0.27.2",
    "dotenv": "16.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/eslint-parser": "7.18.2",
    "azure-functions-core-tools": "4.0.4629",
    "cross-env": "7.0.3",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.7.1",
    "prettier-eslint": "15.0.1"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "commonjs": false,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "import",
      "prettier"
    ],
    "ignorePatterns": [
      "**/.history/*",
      "**/node_modules/*"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 11,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2
  }
}
